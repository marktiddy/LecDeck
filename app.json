{
  "expo": {
    "name": "LecDeck",
    "slug": "LecDeck",
    "platforms": ["ios", "android", "web"],
    "version": "1.96",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#212121"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "./node_modules/@expo/vector-icons/AntDesign.js",
      "./node_modules/@expo/vector-icons/FontAwesome5.js",
      "./node_modules/@expo/vector-icons/Ionicons.js",
      "./node_modules/@expo/vector-icons/FontAwesome.js",
      "./node_modules/moment",
      "./assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mtmedia.lecdeck",
      "buildNumber": "1.7"
    },
    "android": {
      "package": "com.mtmedia.lecdeck",
      "versionCode": 23,
      "compileSdkVersion": 33,
      "targetSdkVersion": 33,
      "buildToolsVersion": "33.0.0",
      "kotlinVersion": "1.9.0",
      "release": {
        "buildType": "apk",
        "image": "latest"
      }
    },
    "extra": {
      "eas": {
        "projectId": "1309c285-9f08-4a7c-8686-5b3fc5698d28"
      }
    },
    "plugins": ["expo-build-properties", "sentry-expo"]
  }
}
